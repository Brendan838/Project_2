<div id = "div1">

    <div id = "div2">

 <h1 id="snipHead">Saved Snippets</h1>
        <button class="saveButton folders"> <a href= "/snips">New Snippet</a> </button>
         {{#each postData as |post|}}
        <button class="saveButton folders"> <a href = "/snippet/{{post.id}}">{{post.title}}</a></button>
         {{/each}}
    </div>


    <div id = "div3">
    <h1 id = "codeEnterHead" >Enter Code Here  <button class="saveButton logOuts" id="logout2">Logout </button></h1>


    <section class= "flex-Column">
	<textarea class="newCode" rows="1" columns="50" id= "snippetTitle">{{populateText.title}}</textarea>
    <textarea class="newCode" rows="9" columns="50" id="snippetBody"> {{populateText.saved_code}}</textarea>
    </section>

    <button class="saveButton" id="updateBtn"> <a href= >Update</a> </button>
    <button class="saveButton" id="deleteBtn"> <a href= >Delete</a> </button>
  

    </div>

</div>

<script type="text/javascript" src="/js/edit.js"></script>